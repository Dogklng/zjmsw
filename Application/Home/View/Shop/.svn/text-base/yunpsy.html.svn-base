</body>
<script type="text/javascript">
  $(function(){
    $('.fukuan').click(function(){
      //console.log($(this).data('id'));
    var ordid = $(this).data('id');
      if(ordid == undefined){
        //获取复选框的订单id
        if($("input[name='tx[]']:checked").length < 1){
          alert('没有提现订单');return false;
        }
        var ordid = '';
        $("input[name='tx[]']:checked").each(function(){
            ordid = ordid + $(this).val() + ",";
        })
        console.log(ordid);
      }
      $("#ordid").val(ordid);
      //console.log($("#ordid").val());
      $('.task').stop(true).fadeIn(1000);
      setTimeout(function(){
          $('.task_con').addClass('task_con_on');
      },300)
  })
$('.task_con>img').click(function(){
      $('.task').stop(true).fadeOut(1000);
      $('.task_con').removeClass('task_con_on');
})
$('.task_btn1_quxiao').click(function(){
      $('.task').stop(true).fadeOut(1000);
      $('.task_con').removeClass('task_con_on');
})
    
    $('#fahuo').click(function(){
       var bid = $('#bankId').val();
       var oid = $('#ordid').val();
        console.log(bid,oid);
     //dialog.showTips('是否提现','firm',function(){
       
        $.ajax({
          url:'/Home/Shop/withd/',
          type:'post',
          data:{oid:oid,bid:bid},
          dataType:'json',
          success:function(g){
              //console.log(g);return false;
              $('.task').stop(true).fadeOut(1000);
              $('.task_con').removeClass('task_con_on');
              $('.wap_tanc_con').css('z-index','9999');
              if(g.status == 1){
                dialog.showTips(g.info,'warn',function(){
                  window.location.reload();
                })
              }else{
                alert(g.info);
              }
          }
        })
     //})
    })
    var txleng = $("input[name='tx[]']:checked").length;
        var tx = $("input[name='tx[]']").length;
        console.log(txleng,tx);
    if(tx == 0){
      $('#tx1').attr('disabled',true);
      $('#vvv').removeClass('fukuan');
      $('#vvv').attr('disabled',true);
    }
    if(txleng < tx){
       $('#tx1').prop('checked',false);
    }else{
      $('#tx1').prop('checked',true);
    }
    $("input[name='tx[]']").click(function(){
        var txleng = $("input[name='tx[]']:checked").length;
        var tx = $("input[name='tx[]']").length;
        console.log(txleng,tx);
        if(txleng < tx){
           $('#tx1').prop('checked',false);
        }else{
          $('#tx1').prop('checked',true);
        }
    })
    
      $('#tx1').click(function(){

        
        //var a = $('#tx').attr('checked');
        //console.log(a);
        //alert(a);
        //console.log(this.checked);
        if($(this).is(':checked')){
          $("input[name='tx[]']").each(function(){
              $(this).prop("checked",true);  
          });
        }else{
           $("input[name='tx[]']").each(function(){
              $(this).prop("checked",false);  
          });
        }
      
      })

    


  })
</script>
