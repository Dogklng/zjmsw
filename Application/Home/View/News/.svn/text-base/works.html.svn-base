<script>

$(document).ready(function() {

	$(".wx").hover(function(){
		$(".ewm1").stop().animate({"opacity":"1"},500);
		    
	},function(){
		$(".ewm1").stop().animate({"opacity":"0"},500);
		    
	})
	
	$(".sj").hover(function(){
		$(".ewm2").stop().animate({"opacity":"1"},500);
		    
	},function(){
		$(".ewm2").stop().animate({"opacity":"0"},500);
		    
	})
	
	
	
});

</script>
