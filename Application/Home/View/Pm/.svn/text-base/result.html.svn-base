<!--  / warpper  -->
<div class="warpper wrapper1 wrapper_s1">
  <div class="title">
    <div class="line"></div>
    <p class="font_tit1">Auction results</p>
    <h5>拍 • 卖 • 结 • 果</h5>
  </div>
  <div class="main">
    <div class="row">
      <div class="col-lg-3 col-md-3 col-sm-3">
        <div class="mainList">
          <div class="mainL-calendar">
            <!--<h3 class="mainL-h3">按时间筛选</h3>
            <input class="datainp" id="dateinfo" type="text" placeholder="请选择"  readonly>
            <i><img src="__IMAGES__/xiajiantou.png" onclick="dateinfo()" alt="" /></i> --></div>
          <div class="mainL-screen">
            <h3 class="mainL-h3">按类目筛选<a href="#" onclick="disfun()" class="mainLs-A">取消选中</a></h3>
            <ul class="mainL-screenUl">
              <foreach name="screens" item="v">
                <li>
                  <input type="checkbox" name="screenS[]" id="screenS{$key}"
                  <foreach name="cate" item="vv">
                    <if condition="($v.id eq $vv)">
                      checked="checked"
                      <else/>
                    </if>
                  </foreach> onclick="fun(this)" value="{$v.id}" />
                  <label for="screenS{$key}">{$v.classname}</label>
                </li>
              </foreach>
            </ul>
          </div>
        </div>
      </div>
      <div class="col-lg-9 col-md-9">
        <div class="event_list box">
          <ul>
            <foreach name="num" item="vo">
              <if condition="$key eq 0">
                <li class="first">
                <else/>
                <li>
              </if>

                <div class="pMd">
                  <a href='{:U("Home/Pm/resultNum/num_id")}/{$vo.id}' target="_blank" class="image">
                    <img src="{$vo.backg_pic}" style="height: 120px;">
                  </a>
                  <a href='{:U("Home/Pm/resultNum/num_id")}/{$vo.id}' target="_blank" class="names">{$vo.name}</a>
                  <span class="action">
                  <em class="price">￥ {$vo.pm.dangqian} </em>
                  </span><span class="time">{$vo.start_time|date="Y年m月d日 H:i",###} - {$vo.end_time|date="Y年m月d日 H:i",###}</span> <span class="date">
                  <span class="day">{$vo.start_time|date="d",###}</span><span class="month">
                  <switch name="vo.start|date='m',###">
                    <case value="01">一</case>
                    <case value="02">二</case>
                    <case value="03">三</case>
                    <case value="04">四</case>
                    <case value="05">五</case>
                    <case value="06">六</case>
                    <case value="07">七</case>
                    <case value="08">八</case>
                    <case value="09">九</case>
                    <case value="10">十</case>
                    <case value="11">十一</case>
                    <case value="12">十二</case>
                  </switch>
                  月</span><span class="year">{$vo.start_time|date="Y",###}</span>
              </span> <span class="count"><span class="bid">{$vo.zcj|default=0}</span><b>出价次数</b><span class="views">{$vo.wgcont|default=0}</span><b>围观数</b></span> </div>
              </li>
            </foreach>
          </ul>
          <!--<p class="pagination">{$page}</p>-->
        </div>
      </div>
    </div>
  </div>
  <div class="main_page">
    {$page}
  </div>
  <div class="top"> <a class="top_a" href="javascript:goTop();"> <img src="__IMAGES__/top.png"> </a> </div>
</div>
<!--  / warpper  -->

