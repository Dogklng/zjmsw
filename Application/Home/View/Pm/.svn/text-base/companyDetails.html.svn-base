</body>
 
 <script>
 	
$(window).ready(function(){
$(".announce_top").animate({"bottom":"0px","opacity":"1"},1000);
})

 
$(window).scroll(function(){ 


if($(window).scrollTop()>($(".announce_box").offset().top-500)){
$(".presenting").animate({"bottom":"0px","opacity":"1"},1000);
var length=$(".contlist").length;
		for(i=0;i<=length;i++){
			(function(){
				var c=i;
				var time=100+c*200;
				$(".contlist").eq(i).delay(time).animate({"opacity":"1","bottom":"0"},1000)
			})()
		}
		
		
	}

})



 </script>

