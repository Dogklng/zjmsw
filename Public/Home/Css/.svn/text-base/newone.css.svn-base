@media screen and (max-width:1024px) {
	[data-menu=toggleTarget] {
		position: absolute;
		overflow-y: auto;
		overflow-x: hidden;
		width: 100%;
		-webkit-transition: top 1s;
		transition: top 1s;
		max-width: 100%;
		margin: 0;
		padding: 0
	}
	[data-menu-item-target] {
		position: absolute;
		z-index: 200;
		width: 100%;
		top: 0;
		-webkit-transform: translateX(200%);
		-ms-transform: translateX(200%);
		transform: translateX(200%);
		-webkit-transition: -webkit-transform .4s;
		transition: -webkit-transform .4s;
		transition: transform .4s;
		transition: transform .4s, -webkit-transform .4s
	}
	[data-menu-item-target].moveLeft {
		-webkit-transform: translateX(100%);
		-ms-transform: translateX(100%);
		transform: translateX(100%)
	}
	[data-menu-item-target].moveLeftSublevel {
		-webkit-transform: translateX(-100%);
		-ms-transform: translateX(-100%);
		transform: translateX(-100%)
	}
	[data-menu=subLevel] {
		position: absolute;
		width: 100%;
		top: 0;
		-webkit-transform: translateX(300%);
		-ms-transform: translateX(300%);
		transform: translateX(300%);
		-webkit-transition: -webkit-transform .4s;
		transition: -webkit-transform .4s;
		transition: transform .4s;
		transition: transform .4s, -webkit-transform .4s;
		background-color: #fff
	}
	[data-menu=subLevel].moveLeft {
		-webkit-transform: translateX(200%);
		-ms-transform: translateX(200%);
		transform: translateX(200%)
	}
	[data-menu=firstLevel] {
		position: absolute;
		width: 100%;
		height: 100%;
		top: 62px;
		-webkit-transform: translateX(0);
		-ms-transform: translateX(0);
		transform: translateX(0);
		-webkit-transition: -webkit-transform .4s;
		transition: -webkit-transform .4s;
		transition: transform .4s;
		transition: transform .4s, -webkit-transform .4s
	}
	[data-menu=firstLevel].moveLeft {
		-webkit-transform: translateX(-100%);
		-ms-transform: translateX(-100%);
		transform: translateX(-100%)
	}
	[data-menu-item] [class*=icon-],
	[data-menu-item] [class^=icon-],
	[data-menu=nextLevel] [class*=icon-],
	[data-menu=nextLevel] [class^=icon-] {
		position: absolute;
		top: 50%;
		right: 20px;
		-webkit-transform: translateY(-50%);
		-ms-transform: translateY(-50%);
		transform: translateY(-50%)
	}
}

