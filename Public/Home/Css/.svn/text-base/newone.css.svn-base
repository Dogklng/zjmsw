select.selector {
	width: 15.625rem;
	padding: 20px 0 15px 20px;
	padding: 1.25rem 0 .9375rem 1.25rem;
	background-color: #fff;
	border: 1px solid #e5e5e5;
	display: inline-block;
	font: inherit;
	margin: 0;
	box-sizing: border-box;
	-webkit-appearance: none;
	-moz-appearance: none;
	background-image: -webkit-linear-gradient(45deg, transparent 50%, #000 0), -webkit-linear-gradient(315deg, #000 50%, transparent 0);
	background-image: linear-gradient(45deg, transparent 50%, #000 0), linear-gradient(135deg, #000 50%, transparent 0);
	background-position: calc(100% - 20px) calc(1em + 8px), calc(100% - 15px) calc(1em + 8px), calc(100% - 2.5em) .5em
}

select.selector,
select.selector:focus {
	background-size: 5px 5px, 5px 5px, 1px 1.5em;
	background-repeat: no-repeat
}

select.selector:focus {
	background-image: -webkit-linear-gradient(45deg, #000 50%, transparent 0), -webkit-linear-gradient(315deg, transparent 50%, #000 0);
	background-image: linear-gradient(45deg, #000 50%, transparent 0), linear-gradient(135deg, transparent 50%, #000 0);
	background-position: calc(100% - 15px) calc(1em + 8px), calc(100% - 20px) calc(1em + 8px), calc(100% - 2.5em) .5em;
	border-color: #e5e5e5;
	outline: 0
}

select.selector:-moz-focusring {
	color: transparent;
	text-shadow: 0 0 0 #000
}

select.selector::-ms-expand {
	display: none
}

select.selector option {
	margin: 10px;
	margin: .625rem
}

