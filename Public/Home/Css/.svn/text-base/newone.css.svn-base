.checkboxGroup input {
	position: absolute;
	left: -9999px
}

.checkboxGroup label {
	display: inline-block;
	position: relative;
	padding-left: 30px;
	padding-left: 1.875rem;
	cursor: pointer
}

.checkboxGroup label:before {
	display: block;
	content: "";
	position: absolute;
	width: 19px;
	height: 19px;
	left: 0;
	top: 0;
	border: 1px solid #000
}

.checkboxGroup label:after {
	display: none;
	content: "\e911";
	position: absolute;
	top: -1px;
	left: 3px;
	font-family: msr-icon-font!important;
	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale
}

.checkboxGroup input:checked+label:after {
	display: block
}

